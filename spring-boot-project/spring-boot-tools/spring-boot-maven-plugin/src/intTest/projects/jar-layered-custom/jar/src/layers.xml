<layers-configuration xmlns="http://www.springframework.org/schema/layers"
					  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
					  xsi:schemaLocation="http://www.springframework.org/schema/layers
					  https://www.springframework.org/schema/layers/layers-configuration.xsd">
	<layers>
		<layer>configuration</layer>
		<layer>application</layer>
		<layer>snapshot-dependencies</layer>
		<layer>my-dependencies-name</layer>
	</layers>
	<libraries>
		<default-strategy id="snapshot-dependencies"/>
		<default-strategy id="dependencies" layer="my-dependencies-name"/>
	</libraries>
	<classes>
		<strategy layer="configuration">
			<names>
				<include>application*.*</include>
			</names>
		</strategy>
		<default-strategy id="application"/>
	</classes>
</layers-configuration>