<layers-configuration xmlns="http://www.springframework.org/schema/layers"
					  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
					  xsi:schemaLocation="http://www.springframework.org/schema/layers
					  https://www.springframework.org/schema/layers/layers-configuration.xsd">
	<layers>
		<layer>snapshots</layer>
		<layer>my-deps</layer>
	</layers>
	<libraries>
		<!-- The default strategy for snapshots-->
		<default-strategy id="snapshot-dependencies"/>
		<!-- Strategy for including dependencies with given coordinates-->
		<default-strategy id="my-deps" layer="my-deps"/>
		<!-- Default dependencies strategy with layer name renamed-->
	</libraries>
</layers-configuration>